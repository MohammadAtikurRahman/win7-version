{
    "name": "D-Lab",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@date-io/date-fns": "^2.16.0",
        "@material-ui/core": "^4.11.2",
        "@material-ui/icons": "^4.9.1",
        "@material-ui/lab": "^4.0.0-alpha.57",
        "@material-ui/pickers": "^3.3.10",
        "@mui/lab": "^5.0.0-alpha.136",
        "@version/react-router-v3": "^1.0.0",
        "axios": "^0.21.0",
        "bcrypt": "^5.0.0",
        "bcryptjs": "^2.4.3",
        "concurrently": "^5.3.0",
        "cors": "^2.8.5",
        "crypto-browserify": "^3.12.0",
        "dotenv": "^16.3.1",
        "electron": "^14.2.9",
        "file-saver": "^2.0.5",
        "fs": "^0.0.1-security",
        "fuzzy-search": "^3.2.1",
        "getos": "^3.2.1",
        "json2csv": "^5.0.7",
        "jsonwebtoken": "^9.0.0",
        "jwt-decode": "^3.1.2",
        "material-ui": "^0.15.0",
        "moment": "^2.29.4",
        "papaparse": "^5.4.1",
        "react": "^16.13.1",
        "react-bootstrap-sweetalert": "^5.2.0",
        "react-datepicker": "^4.8.0",
        "react-dom": "^16.13.1",
        "react-dotenv": "^0.1.3",
        "react-hook-form": "^7.45.1",
        "react-router-dom": "^6.4.5",
        "react-scripts": "^5.0.1",
        "sweetalert": "^2.1.2",
        "sweetalert2": "^11.7.0",
        "video.js": "^8.3.0"
    },
    "main": "public/electron.js",
    "scripts": {
        "start": "concurrently \"npm run start-electron\" \"npm run start-react\"",
        "start-react": "cross-env BROWSER=none react-scripts start",
        "start-electron": "electron .",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "electron-pack": "electron-builder --dir",
        "electron-build": "electron-builder"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "browser": {
        "crypto": false
    },
    "devDependencies": {
        "cross-env": "^7.0.3",
        "cross-spawn": "^7.0.3",
        "electron-builder": "^23.6.0",
        "npm-run-all": "^4.1.5",
        "wait-on": "^7.0.1"
    },
    "build": {
        "appId": "com.example.my-electron-app",
        "files": [
            "build/**/*",
            "public/electron.js",
            "backend/**/*"
        ],
        "directories": {
            "buildResources": "assets"
        },
        "extraMetadata": {
            "main": "public/electron.js",
            "NODE_ENV": "production"
        },
        "win": {
            "target": [
                "nsis"
            ]
        },
        "nsis": {
            "oneClick": false,
            "allowToChangeInstallationDirectory": true,
            "installerIcon": "./assets/icon/icon.ico",
            "uninstallerIcon": "./assets/icon/icon.ico",
            "installerHeaderIcon": "./assets/icon/icon.ico",
            "createDesktopShortcut": "always",
            "runAfterFinish": false
        }
    }
}
